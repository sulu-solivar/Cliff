<h2>Complete the registration process</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  
  <p><%= f.label :first_name %>
  <%= f.text_field :first_name, :value => session["omniauth"][:info][:first_name] %></p>

  <p><%= f.label :last_name %>
  <%= f.text_field :last_name,:value => session["omniauth"][:info][:last_name] %></p>

  <p><%= f.label :email %>
  <%= f.text_field :email, placeholder: "Enter valid email" %></p>


  <p><%= f.label :highest_education %>
  <%= f.select :highest_education,[
    ["High School Diploma","High School Diploma"],
["Associate Degree","Associate Degree"],
["Bachelors Degree","Bachelors Degree"],
["Masters Degree","Masters Degree"],
["Doctoral Degree","Doctoral Degree"]
    ] %></p>
    <p><%= f.label :years_in_business %>
  <%= f.text_field :years_in_business %></p>

  <p><%= f.label :founded_company %>
  <%= f.check_box :founded_company %></p>

  <p><%= f.label :raised_capital %>
  <%= f.check_box :raised_capital %></p>

  <p><%= f.label :pitched_to_investors %>
  <%= f.check_box :pitched_to_investors %></p>
  <p><%= f.submit "Update" %></p>
<% end %>

